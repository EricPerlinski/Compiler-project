/* Un exemple de programme trÃ¨s simple */
do
	integer i, j, maximum
	
	function int fMax (x: integer, y: integer)
		integer result
		if x > y then result = x
			else result = y
		return (result)
	end /* function */

	procedure theEnd ()
		write "that's all !"
	end
	
	i = -3
	j = 0
	maximum = fMax(i, j)
	write maximum
	theEnd()
end

